#include <iostream>
using namespace std;

int main() {
	
	int t;
	cin>>t;
	while(t--)
	{
	    float x1,x2,y1,y2;
	    cin>>x1>>x2>>y1>>y2;
	    
	    float z1=x1/y1;
	    float z2=x2/y2;
	    
	    if(z1>z2)
	    cout<<-1<<endl;
	    else if(z1<z2)
	    cout<<1<<endl;
	    else
	    cout<<0<<endl;
	}
	return 0;
}
