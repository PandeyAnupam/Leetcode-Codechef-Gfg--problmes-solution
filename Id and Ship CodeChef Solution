#include <iostream>
using namespace std;

int main() {
	int testcase;
	cin>>testcase;
	for(int i=0;i<testcase;i++)
	{
	   char ch;
	   cin>>ch;
	   if(ch=='B'||ch=='b')
	   {
	       cout<<"BattleShip"<<endl;
	   }
	   else if(ch=='c'||ch=='C')
	   {
	       cout<<"Cruiser"<<endl;
	   }
	   else if(ch=='d'||ch=='D')
	   {
	       cout<<"Destroyer"<<endl;
	   }
	   else if(ch=='f'||ch=='F')
	   {
	       cout<<"Frigate"<<endl;
	   }
	}
	return 0;
}
