class Solution {
    // Function to insert a node in a BST.
    Node insert(Node root, int Key) {
        // your code here
        Node newnode= new Node(Key);
        
        if(root==null)
        {
            root=newnode;
            return root;
        }
        if(Key<root.data)
        root.left=insert(root.left,Key);
        else if(Key==root.data);
        else
        root.right=insert(root.right,Key);
        return root;
    }
}
