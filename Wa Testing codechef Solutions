#include <iostream>
using namespace std;

int main() {
	
	int t;
	cin>>t;
	while(t--)
	{
	    int n;
	    cin>>n;
	    int arr[n];
	    for(int i=0;i<n;i++)
	    cin>>arr[i];
	    string str;
	    cin>>str;
	    int max=101;
	    for(int i=0;i<n;i++)
	    {
	        char ch=str[i];
	        if(ch=='0' && max>arr[i])
	        max=arr[i];
	    }
	    cout<<max<<endl;
	}
	return 0;
}
