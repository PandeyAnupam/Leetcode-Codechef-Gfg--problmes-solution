#include <iostream>
using namespace std;

int main() {
	int t;
	cin>>t;
	while(t--)
	{
	    string str1,str2;
	    cin>>str1;
	    cin>>str2;
	    
	    int len=str1.length();
	    for(int i=0;i<len;i++)
	    {
	        if(str1[i]==str2[i])
	        cout<<"G";
	        else
	        cout<<"B";
	    }
	    cout<<endl;
	}
	return 0;
	
}
